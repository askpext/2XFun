{
  "manifest_version": 3,
  "name": "2XFun",
  "version": "1.0",
  "description": "Automatically sets YouTube video playback speed to 2x",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "assets/2x_16.png",  
    "32": "assets/2x_32.png",
    "48": "assets/2x_48.png",  
    "128": "assets/2x_128.png"
  }
}
